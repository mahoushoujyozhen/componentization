{
	"name": "mahoushoujyosys",
	"version": "0.0.1",
	"scripts": {
		"must-run-before-prepare": "npx smui-theme template src/theme",
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-light": "smui-theme compile src/theme/dist/smui.css -i src/theme -i ./node_modules",
		"smui-theme-dark": "smui-theme compile src/theme/dist/smui-dark.css -i src/theme/dark -i ./node_modules",
		
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.25.0",
		"@smui/button": "^6.1.4",
		"@smui/list": "^6.1.0",
		"@smui/snackbar": "^6.1.0",
		"@smui/tab": "^6.1.0",
		"@smui/tab-bar": "^6.1.0",
		"@smui/textfield": "^6.1.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.2.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module"
}
